{
    "name": "full-stack",
    "version": "1.0.0",
    "description": "T-MERN App",
    "main": "dist/server.js",
    "scripts": {
        "build-prod": "npm install && npm run build && ts-node deleteDev.ts && rimraf deleteDev.ts && npm prune --production",
        "heroku-postbuild": "npm run build-prod",
        "prebuild": "rimraf ./dist/*",
        "build": "webpack --config=webpack.config.prod.js",
        "build-dev": "webpack --progress --display-modules --config=webpack.config.dev.js",
        "start": "npm run serve",
        "serve": "node dist/server.js",
        "watch-frontend": "webpack-dev-server --open --stack=frontend --config=webpack.config.dev.js",
        "watch-backend": "nodemon --config \"./nodemon.json\"",
        "watch": "concurrently -k -p \"[{name}]\" -n \"React,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-frontend\" \"npm run watch-backend\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "keywords": [
        "Typescript",
        "MongoDB",
        "ExpressJS",
        "React",
        "NodeJS"
    ],
    "author": "DBC",
    "license": "ISC",
    "dependencies": {
        "@overnightjs/core": "^1.6.9",
        "@overnightjs/logger": "^1.1.9",
        "bcryptjs": "^2.4.3",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "connect-mongo": "^3.0.0",
        "cookie-parser": "^1.4.4",
        "dotenv": "^8.0.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.16.2",
        "lodash": "^4.17.14",
        "lusca": "^1.6.1",
        "mongoose": "^5.6.4",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "path": "^0.12.7",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/bluebird": "^3.5.27",
        "@types/body-parser": "^1.17.0",
        "@types/compression": "0.0.36",
        "@types/connect-mongo": "0.0.42",
        "@types/cookie-parser": "^1.4.1",
        "@types/dotenv": "^6.1.1",
        "@types/errorhandler": "0.0.32",
        "@types/express": "^4.17.0",
        "@types/express-session": "^1.15.13",
        "@types/jest": "^24.0.15",
        "@types/lodash": "^4.14.136",
        "@types/lusca": "^1.6.0",
        "@types/mongodb": "^3.1.28",
        "@types/mongoose": "^5.5.8",
        "@types/passport": "^1.0.0",
        "@types/passport-local": "^1.0.33",
        "@types/react": "^16.7.18",
        "@types/react-dom": "^16.0.11",
        "@types/react-router": "^5.0.3",
        "@types/react-router-dom": "^4.3.1",
        "@types/shelljs": "^0.8.5",
        "@types/supertest": "^2.0.8",
        "@types/winston": "^2.4.4",
        "concurrently": "^4.1.1",
        "css-loader": "^3.0.0",
        "file-loader": "^4.0.0",
        "fork-ts-checker-notifier-webpack-plugin": "^1.0.2",
        "fork-ts-checker-webpack-plugin": "^1.3.7",
        "glob": "^7.1.4",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^5.0.0",
